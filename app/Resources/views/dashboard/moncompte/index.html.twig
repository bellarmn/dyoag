{% extends 'dashboard/layout.html.twig' %}

{% block  title %} Mon Compte {% endblock %}
{% block  menuentete %}
    <!--Tabs-->

{% endblock %}
{% block  corps %}
    <div class="row" style="min-height: 450px;">
        <div class="col-sm-4 col-md-3 user-details" style="margin-top: -8%;">
            <div class="user-image">
                <img src="http://www.gravatar.com/avatar/2ab7b2009d27ec37bffee791819a090c?s=100&d=mm&r=g" alt="Karan Singh Sisodia" title="Karan Singh Sisodia" class="img-circle">
            </div>
            <div class="user-info-block">
                <div class="user-heading">
                    <h3>Karan Singh Sisodia</h3>
                    <span class="help-block">Chandigarh, IN</span>
                </div>
                <ul class="navigation">
                    <li class="active">
                        <a data-toggle="tab" href="#information">
                            <span class="fa fa-user"></span>
                        </a>
                    </li>
                    <li>
                        <a data-toggle="tab" href="#settings">
                            <span class="fa fa-cog"></span>
                        </a>
                    </li>
                    <li>
                        <a data-toggle="tab" href="#email">
                            <span class="fa fa-envelope"></span>
                        </a>
                    </li>
                    <li>
                        <a data-toggle="tab" href="#events">
                            <span class="fa fa-calendar"></span>
                        </a>
                    </li>
                </ul>
                <div class="user-body">
                    <div class="tab-content">
                        <div id="information" class="tab-pane active">
                            <h6>Account Information</h6>
                        </div>
                        <div id="settings" class="tab-pane">
                            <h6>Settings</h6>
                        </div>
                        <div id="email" class="tab-pane">
                            <h6>Send Message</h6>
                        </div>
                        <div id="events" class="tab-pane">
                            <h6>Events</h6>
                        </div>
                    </div>
                </div>
            </div>
        </div>
         <div class="col-md-9">
             {% block  moncompte %} CONTENU A IMPLEMENTER{% endblock %}
         </div>
    </div>
{% endblock  corps %}

{% block javascriptsMarket %}
    <script>
        require(['dashboard/moncompte']);
    </script>
{% endblock %}